{
  "env": {
    "commonjs": true,
    "es2021": true,
    "node": true,
    "jest": true
  },
  "extends": ["eslint:recommended", "plugin:@typescript-eslint/recommended"],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": 12
  },
  "plugins": ["@typescript-eslint"],
  "rules": {
    "no-empty": "warn",
    "no-debugger": "warn",
    "no-unused-labels": "warn",
    "no-useless-catch": "warn",
    "no-octal": "warn"
  }
}
